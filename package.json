{
  "name": "adapta-browser",
  "version": "1.0.0",
  "description": "Browser with semantic similarity search",
  "main": "main.js",
  "scripts": {
    "start": "G_MESSAGES_DEBUG='' electron . --no-sandbox 2>&1 | grep -v 'GLib-GObject'",
    "start:clean": "electron . --no-sandbox",
    "dev": "electron . --no-sandbox --enable-logging",
    "bun:install": "cd bun-workers && bun install",
    "bun:check": "bun --version"
  },
  "keywords": [
    "browser",
    "embeddings",
    "similarity"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0"
  },
  "dependencies": {
    "@huggingface/transformers": "^3.7.6",
    "@mozilla/readability": "^0.6.0",
    "duckdb": "^1.4.1",
    "jsdom": "^24.1.0",
    "simple-statistics": "^7.8.0",
    "yahoo-finance2": "^3.10.2"
  }
}
